public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts)
{
    // code here
    List<sObject> lList = new List<sObject>();
    if(accounts.isEmpty() || accounts==null ){
        return accounts;
    }
    else if(contacts.isEmpty() || contacts== null){
        return contacts;
    }
    for(Account a : accounts){
        lList.add(a);
    }
    for(Contact c : contacts){
        lList.add(c);
    }
    return lList;
}